PLATFORM_NAMESPACE=store-platform

# Rate Limiting
MAX_STORES_PER_USER=10
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Store defaults
DEFAULT_DOMAIN_SUFFIX=.local.stores.dev
